@model System.Collections.Generic.IEnumerable<stunning_robot_HR.Models.Staff>

@{
    ViewData["Title"] = "GetStaffByFullName";
}




@foreach (var item in Model) {
            <dl class="row">
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(modelItem => item.FullName)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(modelItem => item.FullName)
                </dd>
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(modelItem => item.DateOfBirth)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(modelItem => item.DateOfBirth)
                </dd>
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(modelItem => item.Position)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(modelItem => item.Position)
                </dd>
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(modelItem => item.StartDate)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(modelItem => item.StartDate)
                </dd>
            </dl>    
<div>
    <a asp-action="Edit" asp-route-id="@item.StaffId">Edit</a> |
    <a asp-action="Details" asp-route-id="@item.StaffId">Details</a> |
    <a asp-action="Delete" asp-route-id="@item.StaffId">Delete</a>
</div>
        
} 